{% set limits = [5, 10, 50, 100, 500] %}

<div>
    Items per page:
    {% for limit in limits %}
        {% if limit == app.request.session.get('limit', 10) %}
            <b>{{ limit }}</b>
        {% else %}
            <a href="{{ path(app.request.attributes.get('_route'), { limit: limit }) }}">{{ limit }}</a>
        {% endif %}
    {% endfor %}
</div>
